{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.mc.creaking.attack" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"transitions" : [
						{ 
							"attack" : "variable.attack_time > 0.0"
						}
					]
				},
				"attack" : {
					"animations" : [ "attack" ],
					"transitions" : [
						{ 
							"default" : "variable.attack_time <= 0.0"
						}
					]
				}
			}
		}
	}
}
